
<!DOCTYPE html>
<html>

<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <title>Huellitas Vet</title>
    <link rel="icon" href="~/assets/img/brand/huellitas_logo_sinfondo.png" type="image/png">
    <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Open+Sans:300,400,600,700">
    <link rel="stylesheet" href="~/assets/vendor/nucleo/css/nucleo.css" type="text/css">
    <link rel="stylesheet" href="~/assets/vendor/fontawesome/fontawesome-free/css/all.min.css" type="text/css">
    <link rel="stylesheet" href="~/assets/css/argon.css?v=1.2.0" type="text/css">
</head>

<body>
    <nav class="sidenav navbar navbar-vertical  fixed-left  navbar-expand-xs navbar-light bg-white" id="sidenav-main">
        <div class="scrollbar-inner">
            <div class="sidenav-header  align-items-center mt-2 mb-3">
                <a class="" href="@Url.Action("Inicio", "Home")">
                    <img src="~/assets/img/brand/huellitas_logo_sinfondo.png" class="" alt="..." width="100" height="100">
                </a>
            </div >
            <div class="navbar-inner">
                <div class="collapse navbar-collapse" id="sidenav-collapse-main">
                    <ul class="navbar-nav">
                        <li class="nav-item">
                            <a class="nav-link active" href="@Url.Action("Inicio", "Home")">
                                <i class="ni ni-tv-2 text-primary"></i>
                                <span class="nav-link-text">Inicio</span>
                            </a>
                        </li>

                        <li class="nav-item">
                            <a class="nav-link" href="examples/icons.html">
                                <i class="ni ni-single-02 text-green"></i>
                                <span class="nav-link-text">Clientes</span>
                            </a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link collapsed" href="#navbar-cliente" data-toggle="collapse" role="button" aria-expanded="false" aria-controls="navbar-cliente">
                                <i class="ni ni-single-02 text-green"></i>
                                <span class="nav-link-text">Usuarios</span>
                            </a>
                            <div class="collapse" id="navbar-cliente">
                                <ul class="nav nav-sm flex-column">
                                    <li class="nav-item">
                                        <a href="@Url.Action("ConsultarUsuarios","Usuario")" class="nav-link">Consultar Usuarios</a>
                                    </li>
                                </ul>
                            </div>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link collapsed" href="#navbar-mascotas" data-toggle="collapse" role="button" aria-expanded="false" aria-controls="navbar-mascotas">
                                <i class="ni ni-favourite-28 text-overflow"></i>
                                <span class="nav-link-text">Mascotas</span>
                            </a>
                            <div class="collapse" id="navbar-mascotas">
                                <ul class="nav nav-sm flex-column">
                                    <li class="nav-item">
                                        <a href="@Url.Action("RegistrarMascota","Mascota")" class="nav-link">Registrar Mascota</a>
                                    </li>
                                    <li class="nav-item">
                                        <a href="@Url.Action("ConsultarMascotas","Mascota")" class="nav-link">Consultar Mascotas</a>
                                    </li>
                                </ul>
                            </div>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" href="examples/map.html">
                                <i class="ni ni-collection text-red"></i>
                                <span class="nav-link-text">Servicios</span>
                            </a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" href="examples/profile.html">
                                <i class="ni ni-calendar-grid-58 text-blue"></i>
                                <span class="nav-link-text">Citas</span>
                            </a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" href="examples/tables.html">
                                <i class="ni ni-single-copy-04 text-purple"></i>
                                <span class="nav-link-text">Informes</span>
                            </a>
                        </li>
                    </ul>
                </div>
            </div>
        </div>
    </nav>
    <div class="main-content" id="panel">
        <nav class="navbar navbar-top navbar-expand navbar-dark bg-primary border-bottom">
            <div class="container-fluid">
                <div class="collapse navbar-collapse" id="navbarSupportedContent">

                    <ul class="navbar-nav align-items-center  ml-md-auto ">
                        <li class="nav-item d-xl-none">
                            <div class="pr-3 sidenav-toggler sidenav-toggler-dark" data-action="sidenav-pin" data-target="#sidenav-main">
                                <div class="sidenav-toggler-inner">
                                    <i class="sidenav-toggler-line"></i>
                                    <i class="sidenav-toggler-line"></i>
                                    <i class="sidenav-toggler-line"></i>
                                </div>
                            </div>
                        </li>
                        <li class="nav-item d-sm-none">
                            <a class="nav-link" href="#" data-action="search-show" data-target="#navbar-search-main">
                                <i class="ni ni-zoom-split-in"></i>
                            </a>
                        </li>
                    </ul>
                    <ul class="navbar-nav align-items-center  ml-auto ml-md-0 ">
                        <li class="nav-item dropdown">
                            <a class="nav-link pr-0" href="#" role="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                                <div class="media align-items-center">
                                    <span class="avatar avatar-sm rounded-circle">
                                        <img alt="Image placeholder" src="~/assets/img/theme/team-4.jpg">
                                    </span>
                                    <div class="media-body  ml-2  d-none d-lg-block">
                                        <span class="mb-0 text-sm  font-weight-bold">John Snow</span>
                                    </div>
                                </div>
                            </a>
                            <div class="dropdown-menu  dropdown-menu-right ">
                                <div class="dropdown-header noti-title">
                                    <h6 class="text-overflow m-0">Bienvenido!</h6>
                                </div>
                                <a href="#!" class="dropdown-item">
                                    <i class="ni ni-single-02"></i>
                                    <span>Mi perfil</span>
                                </a>
                                <a href="@Url.Action("Salir", "Home")" class="dropdown-item">
                                    <i class="ni ni-user-run"></i>
                                    <span>Cerrar Sesión</span>
                                </a>
                            </div>
                        </li>
                    </ul>
                </div>
            </div>
        </nav>
       
       

        <div class="content-wrapper">
            <div>
                @RenderBody()
            </div>
        </div>
        
        <footer class="footer pt-0">
            <div class="row align-items-center justify-content-lg-between">
                <div class="col-lg-6 ml-2">
                    <div class="copyright text-center  text-lg-left  text-muted">
                        Huellitas Vet @DateTime.Now.Year
                    </div>
                </div>

            </div>
        </footer>
    </div>
    <script src="~/assets/vendor/jquery/dist/jquery.min.js"></script>
    <script src="~/assets/vendor/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/assets/vendor/js-cookie/js.cookie.js"></script>
    <script src="~/assets/vendor/jquery.scrollbar/jquery.scrollbar.min.js"></script>
    <script src="~/assets/vendor/jquery-scroll-lock/dist/jquery-scrollLock.min.js"></script>
    <script src="~/assets/vendor/chart.js/dist/Chart.min.js"></script>
    <script src="~/assets/vendor/chart.js/dist/Chart.extension.js"></script> 
    <script src="~/assets/js/argon.js?v=1.2.0"></script>
    <script src="https://cdn.datatables.net/2.0.8/js/dataTables.js"></script>
    <script src="https://cdn.datatables.net/2.0.8/js/dataTables.bootstrap5.js"></script>
    <script src="~/js/Consultas.js"></script>
</body>

</html>
